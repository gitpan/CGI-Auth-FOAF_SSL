	dcs:changeset [
		dcs:versus my:v_1-000 ;
		dcs:item
			[ rdfs:label "Use Crypt::X509, Crypt::OpenSSL::X509, DateTime and DateTime::Format::Strptime to parse PEM-encoded certificates rather than relying on the OpenSSL command-line client."@en ] ,
